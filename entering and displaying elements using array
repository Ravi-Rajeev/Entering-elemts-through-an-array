#include <iostream>
#include<string>
using namespace std;

int main()
{
    int arr[5];
    int i=0;
    cout<<"Enter 5 elements: "<<endl;
    for(i=0;i<5;i++)
    {
        cin>>arr[i];
    }
    cout<<"\n";
    cout<<"The entered elemts are:\n"<<endl;
    for(i=0;i<5;i++)
    {
     cout<<arr[i]<<"\n";    
    }

    return 0;
}
